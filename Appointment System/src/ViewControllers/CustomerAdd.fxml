<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="367.0" prefWidth="410.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox layoutX="2.0" layoutY="49.0" prefHeight="268.0" prefWidth="408.0">
         <children>
            <VBox alignment="CENTER" prefHeight="409.0" prefWidth="131.0" spacing="20.0">
               <children>
                  <Label text="Customer ID" />
                  <Label text="Name" />
                  <Label text="Address" />
                  <Label text="Country" />
                  <Label text="State/Province" />
                  <Label text="Postal Code" />
                  <Label text="Phone Number" />
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="339.0" prefWidth="262.0" spacing="12.0">
               <children>
                  <TextField fx:id="CustomerID" disable="true" editable="false" />
                  <TextField fx:id="Name" />
                  <TextField fx:id="Address" />
                  <ComboBox fx:id="Country" prefHeight="25.0" prefWidth="296.0" />
                  <ComboBox fx:id="StateProvince" prefHeight="25.0" prefWidth="296.0" />
                  <TextField fx:id="PostalCode" />
                  <TextField fx:id="PhoneNumber" />
               </children>
            </VBox>
         </children>
      </HBox>
      <Button layoutX="14.0" layoutY="317.0" mnemonicParsing="false" onMouseClicked="#ReturnClick" prefHeight="37.0" prefWidth="121.0" style="-fx-border-color: red;" text="Cancel">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="275.0" layoutY="317.0" mnemonicParsing="false" onMouseClicked="#SaveClick" prefHeight="37.0" prefWidth="121.0" text="Save">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="35.0" prefWidth="207.0" text="Add Customer">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
